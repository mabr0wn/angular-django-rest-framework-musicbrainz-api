{
  "name": "angular-musicbrainz-api",
  "version": "1.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test angular-musicbrainz-api --coverage --watch=false",
    "test:update": "ng test angular-musicbrainz-api --coverage --watch=false -u",
    "test:watch": "ng test angular-musicbrainz-api --watch=true",
    "test:coverage": "ng test angular-musicbrainz-api --coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run angular-musicbrainz-api:server:production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.0.0",
    "@angular/cdk": "8.0.0",
    "@angular/common": "8.0.0",
    "@angular/compiler": "8.0.0",
    "@angular/core": "8.0.0",
    "@angular/forms": "8.0.0",
    "@angular/http": "^7.2.10",
    "@angular/material": "8.0.0",
    "@angular/platform-browser": "8.0.0",
    "@angular/platform-browser-dynamic": "8.0.0",
    "@angular/platform-server": "8.0.0",
    "@angular/router": "8.0.0",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "0.0.0",
    "core-js": "^3.0.0",
    "express": "^4.15.2",
    "rxjs": "^6.4.0",
    "rxjs-compat": "^6.4.0",
    "webpack-bundle-tracker": "^0.4.2-beta",
    "zone.js": "^0.9.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^7.4.3",
    "@angular-builders/dev-server": "^7.3.1",
    "@angular-builders/jest": "^7.4.2",
    "@angular-devkit/build-angular": "0.800.0",
    "@angular/cli": "8.0.0",
    "@angular/compiler-cli": "8.0.0",
    "@angular/language-service": "8.0.0",
    "@types/jest": "^24.0.12",
    "@types/node": "^12.6.0",
    "codelyzer": "^5.0.0-beta.1",
    "jest": "^24.8.0",
    "jest-preset-angular": "^7.1.0",
    "karma-cli": "^2.0.0",
    "ngx-jest-snapshot-serializer": "^1.2.3",
    "ngx-wallaby-jest": "^0.0.2",
    "protractor": "^6.0.0",
    "rxjs-tslint": "^0.1.7",
    "ts-loader": "^5.2.0",
    "ts-node": "~8.0.3",
    "tslint": "^5.18.0",
    "typescript": "3.2.4",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.1.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "snapshotSerializers": [
      "ngx-jest-snapshot-serializer"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/jest-config/",
      "<rootDir>/node_modules/",
      "<rootDir>/src/app/shared/search/search-params.ts"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist",
      "<rootDir>/node_modules/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest-config/setupJest.ts"
    ],
    "moduleNameMapper": {
      "@core/(.*)": "<rootDir>/src/app/core/$1",
      "@shared/(.*)": "<rootDir>/src/app/shared/$1",
      "@mock/(.*)": "<rootDir>/src/app/__mocks__/$1"
    }
  }
}
