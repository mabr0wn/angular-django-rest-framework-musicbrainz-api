{
  "name": "angular-musicbrainz-api",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run angular-musicbrainz-api:server:production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.10",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.10",
    "@angular/compiler": "^7.2.10",
    "@angular/core": "^7.2.10",
    "@angular/forms": "^7.2.10",
    "@angular/http": "^7.2.10",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^7.2.10",
    "@angular/platform-browser-dynamic": "^7.2.10",
    "@angular/platform-server": "^7.2.10",
    "@angular/router": "^7.2.10",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "0.0.0",
    "core-js": "^3.0.0",
    "express": "^4.15.2",
    "rxjs": "^6.4.0",
    "rxjs-compat": "^6.4.0",
    "webpack-bundle-tracker": "^0.4.2-beta",
    "zone.js": "^0.9.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^7.4.3",
    "@angular-builders/dev-server": "^7.3.1",
    "@angular-builders/jest": "^7.4.2",
    "@angular-devkit/build-angular": "^0.13.6",
    "@angular/cli": "^7.3.6",
    "@angular/compiler-cli": "^7.2.10",
    "@angular/language-service": "^7.2.10",
    "@types/jasmine": "^3.3.12",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^24.0.12",
    "@types/node": "^11.11.6",
    "codelyzer": "^5.0.0-beta.1",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jest": "^24.7.1",
    "karma-cli": "^2.0.0",
    "protractor": "^6.0.0",
    "rxjs-tslint": "^0.1.7",
    "ts-loader": "^5.2.0",
    "ts-node": "~8.0.3",
    "tslint": "^5.16.0",
    "typescript": "3.2.4",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.1.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "coveragePathIgnorePatterns": [
      "<rootDir>/jest-config/",
      "<rootDir>/node_modules/"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist",
      "<rootDir>/node_modules/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest-config/setupJest.ts"
    ],
    "moduleNameMapper": {
      "@core/(.*)": "<rootDir>/src/app/core/$1",
      "@shared/(.*)": "<rootDir>/src/app/shared/$1"
    }
  }
}
